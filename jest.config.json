{
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
        ".js"
    ],
    "transform": {},
    "testMatch": [
        "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
        "src/**/*.js",
        "!src/server.js"
    ],
    "coverageDirectory": "coverage",
    "testTimeout": 30000
}